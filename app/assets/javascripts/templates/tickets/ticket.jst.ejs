<div class="row detailed-info-item">
  <div class="detailed-info-item-label medium-6 columns">
    <%= JST['templates/tickets/date_' + status]({started_at: started_at, due_at: due_at, completed_at: completed_at}) %>
  </div>

  <div class="detailed-info-item-value medium-6 columns">
    <%= JST['templates/tickets/status']({status: status, id: id}) %>
    <% if (manually_created_by) { %>
      <div class="detailed-info-comment">
        (manually created by <a href="mailto:<%= manually_created_by.email %>"> <%= manually_created_by.email %></a>)
      </div>
    <% } %>
    <% if (manually_completed_by) { %>
      <div class="detailed-info-comment">
        (manually completed by <a href="mailto:<%= manually_completed_by.email %>"> <%= manually_completed_by.email %></a>)
      </div>
    <% } %>
    <% if (generated_by_sensor) { %>
      <div class="detailed-info-comment">
        (generated by sensor)
      </div>
    <% } %>
  </div>
</div>
